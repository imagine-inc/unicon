# platform :ios, '9.0'

target 'unicon' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for unicon

   pod 'Firebase/Core'
   pod 'Firebase/Auth'
   pod 'Firebase/Firestore'
   
   # Facebook
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'FacebookShare'

  target 'uniconTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'uniconUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
